-- Settings
local Settings = Window:NewTab("Settings")
local SettingsSection = Settings:NewSection("Settings")


SettingsSection:NewKeybind("ToggleUI", "Toggle The UI On and Off", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
