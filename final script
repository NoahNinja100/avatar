local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Avatar Editor - By Noah", "BloodTheme")

-- Body
local Body = Window:NewTab("Body")
local BodySection = Body:NewSection("Body")


BodySection:NewButton("Right Korblox", "Permanent Do Not Press Twice", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/PewPewBoy/loadstrings/main/korblox%20right%20leg"))()
end)

BodySection:NewButton("Left Korblox", "Permanent Do Not Press Twice", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/PewPewBoy/loadstrings/main/korblox%20left%20leg"))()
end)

BodySection:NewButton("Both Korblox", "Permanent Do Not Press Twice", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/PewPewBoy/loadstrings/main/both%20korblox%20legs"))()
end)

BodySection:NewButton("Headless", "Permanent Do Not Press Twice", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/PewPewBoy/loadstrings/main/headless"))()
end)


